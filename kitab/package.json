{
  "name": "kitab",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "prepare": "panda codegen",
    "build": "remix build",
    "dev": "remix dev --manual -c \"pnpm run start\"",
    "start": "wrangler pages dev --compatibility-date=2023-06-21 ./public",
    "pages:deploy": "pnpm run build && wrangler pages deploy ./public",
