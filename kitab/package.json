    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@ark-ui/react": "^2.1.1",
    "@fontsource-variable/inter": "^5.0.16",
    "@fontsource/arvo": "^5.0.18",
