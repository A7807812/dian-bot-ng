{
  "name": "kitab",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "prepare": "pnpm run \"/^codegen:.*/\"",
    "build": "remix build",
    "dev": "pnpm run --parallel \"/^dev:.*/\"",
    "dev:app": "remix dev --manual -c \"pnpm run start\"",
    "dev:graphql": "graphql-codegen --watch",
    "start": "wrangler pages dev --compatibility-date=2023-06-21 ./public",
    "pages:deploy": "pnpm run build && wrangler pages deploy ./public",
    "format": "biome format ./server.ts ./app --write",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "tsc",
    "codegen:panda": "panda codegen",
    "codegen:graphql": "graphql-codegen"
  },
  "dependencies": {
    "@ark-ui/react": "^2.1.1",
    "@conform-to/react": "^1.0.2",
    "@conform-to/zod": "^1.0.2",
    "@epic-web/client-hints": "^1.3.0",
    "@fontsource-variable/cinzel": "^5.0.8",
    "@fontsource-variable/inter": "^5.0.16",
    "@fontsource/silkscreen": "^5.0.18",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@remix-run/cloudflare": "^2.7.2",
    "@remix-run/cloudflare-pages": "^2.7.2",
    "@remix-run/css-bundle": "^2.7.2",
    "@remix-run/node": "^2.7.2",
    "@remix-run/react": "^2.7.2",
    "@remix-run/serve": "^2.7.2",
    "@urql/core": "^4.2.3",
    "isbot": "^5.1.0",
    "ky": "^1.2.0",
    "lucide-react": "^0.337.0",
    "radash": "^12.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sonner": "^1.4.0",
    "tiny-invariant": "^1.3.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@cloudflare/workers-types": "^4.20240222.0",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.2.4",
    "@pandacss/dev": "^0.32.0",
    "@parcel/watcher": "^2.4.0",
    "@park-ui/panda-preset": "^0.35.2",
    "@remix-run/dev": "^2.7.2",
    "@types/react": "^18.2.58",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "eslint": "^8.56.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "graphql": "^16.8.1",
    "typescript": "^5.3.3",
    "wrangler": "^3.29.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
