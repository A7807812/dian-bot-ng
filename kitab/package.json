    "@remix-run/cloudflare": "^2.6.0",
    "@remix-run/cloudflare-pages": "^2.6.0",
    "@remix-run/css-bundle": "^2.6.0",
    "@remix-run/node": "^2.6.0",
    "@remix-run/react": "^2.6.0",
    "@remix-run/serve": "^2.6.0",
    "isbot": "^5.1.0",
    "lucide-react": "^0.331.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-invariant": "^1.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
